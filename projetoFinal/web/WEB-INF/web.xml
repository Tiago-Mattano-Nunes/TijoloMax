<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>/WEB-INF/applicationContext.xml</param-value>
    </context-param>
    <listener>
        <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>dispatcher</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
        <load-on-startup>2</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>CadastroController</servlet-name>
        <servlet-class>controller.CadastroController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IndexController</servlet-name>
        <servlet-class>controller.IndexController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastroPController</servlet-name>
        <servlet-class>controller.CadastroPController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PUnicoController</servlet-name>
        <servlet-class>controller.PUnicoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Frioscontroller</servlet-name>
        <servlet-class>controller.Frioscontroller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CarrinhoController</servlet-name>
        <servlet-class>controller.CarrinhoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PerfilController</servlet-name>
        <servlet-class>controller.PerfilController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PerfilUsuController</servlet-name>
        <servlet-class>controller.PerfilUsuController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TelaADMController</servlet-name>
        <servlet-class>controller.TelaADMController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FinalCompraController</servlet-name>
        <servlet-class>controller.FinalCompraController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastroCatController</servlet-name>
        <servlet-class>controller.CadastroCatController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SobrenosController</servlet-name>
        <servlet-class>controller.SobrenosController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PagamentoController</servlet-name>
        <servlet-class>controller.PagamentoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartaoCController</servlet-name>
        <servlet-class>controller.CartaoCController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PixController</servlet-name>
        <servlet-class>controller.PixController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartaoDebitoController</servlet-name>
        <servlet-class>controller.CartaoDebitoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AgradecimentoController</servlet-name>
        <servlet-class>controller.AgradecimentoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EstroqueController</servlet-name>
        <servlet-class>controller.EstroqueController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EstoqueController</servlet-name>
        <servlet-class>controller.EstoqueController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VendasController</servlet-name>
        <servlet-class>controller.VendasController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IndexController</servlet-name>
        <url-pattern>/Index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastroController</servlet-name>
        <url-pattern>/Cadastro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastroPController</servlet-name>
        <url-pattern>/CadastroP</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PUnicoController</servlet-name>
        <url-pattern>/PUnico</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Frioscontroller</servlet-name>
        <url-pattern>/Frios</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CarrinhoController</servlet-name>
        <url-pattern>/Carrinho</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PerfilController</servlet-name>
        <url-pattern>/Perfil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PerfilUsuController</servlet-name>
        <url-pattern>/PerfilUsu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TelaADMController</servlet-name>
