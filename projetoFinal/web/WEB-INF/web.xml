        <url-pattern>/Pagamento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CartaoCController</servlet-name>
        <url-pattern>/CartaoC</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PixController</servlet-name>
        <url-pattern>/Pix</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CartaoDebitoController</servlet-name>
        <url-pattern>/CartaoDebito</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AgradecimentoController</servlet-name>
        <url-pattern>/Agradecimento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EstroqueController</servlet-name>
        <url-pattern>/Estroque</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EstoqueController</servlet-name>
        <url-pattern>/Estoque</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VendasController</servlet-name>
        <url-pattern>/Vendas</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>redirect.jsp</welcome-file>
    </welcome-file-list>
</web-app>
